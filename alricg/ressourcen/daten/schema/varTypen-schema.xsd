	<xsd:simpleType name="regionVolkTyp">
		<xsd:annotation>
			<xsd:documentation xml:lang="DE">
				GAR 	- garetien
				HOR, ART - horasreich
				ALB 	- albernien
				ALM 	- almada
				AND 	- andergast (+ Nostria)
				WEI, TOB, SVE - weiden
				ZYK 	- zyklopeninseln
				BOR 	- bornland
				THO 	- thorwal
				RIV   	- gjalskerland
				   		- fjarninger
				NIV 	- nivensen
					   	- norbarden
				MHA, KHU, RAS, GOR, THA - tulamidisch
				KHO		- wüsteKhom
				ARA		- aranien
						- ferkina
						- trollzacker
						- zahori
				WAL, CHA - waldmenschen
				ZWE 	- zwerge
				ORK 	- orks
						- goblins
				ACH		- achaz
				SHI, SKR, MAR - maraskan
				ELF, FIR - elfen
				ALA, BRA, MEN - südlicheStädte
				GAL, RHA, XER, GLO, ORO - schwarzeLande
				EHE 	 - ehernesSchwert
				
				schwarzeLande und ehernesSchwert sind KEINE angaben für sprachen, sondern für Waffen u.ä.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="REG-garetien"/>
			<xsd:enumeration value="REG-horasreich"/>
			<xsd:enumeration value="REG-albernien"/>
			<xsd:enumeration value="REG-almada"/>
			<xsd:enumeration value="REG-andergast"/>
			<xsd:enumeration value="REG-weiden"/>
			<xsd:enumeration value="REG-zyklopeninseln"/>
			<xsd:enumeration value="REG-bornland"/>
			<xsd:enumeration value="REG-thorwal"/>
			<xsd:enumeration value="REG-gjalskerland"/>
			<xsd:enumeration value="REG-fjarninger"/>
			<xsd:enumeration value="REG-nivensen"/>
			<xsd:enumeration value="REG-norbarden"/>
			<xsd:enumeration value="REG-tulamidisch"/>
			<xsd:enumeration value="REG-arnaien"/>
			<xsd:enumeration value="REG-ferkina"/>
			<xsd:enumeration value="REG-trollzacker"/>
			<xsd:enumeration value="REG-zahori"/>
			<xsd:enumeration value="REG-waldmenschen"/>
			<xsd:enumeration value="REG-zwerge"/>
			<xsd:enumeration value="REG-orks"/>
			<xsd:enumeration value="REG-goblins"/>
			<xsd:enumeration value="REG-elfen"/>
			<xsd:enumeration value="REG-maraskan"/>
			<xsd:enumeration value="REG-südlicheStädte"/>
			<xsd:enumeration value="REG-wüsteKhom"/>
			<xsd:enumeration value="REG-schwarzeLande"/>
			<xsd:enumeration value="REG-ehernesSchwert"/>
		</xsd:restriction>
